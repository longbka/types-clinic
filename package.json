{
  "name": "@tclinic/shared",
  "version": "1.0.2",
  "description": "Shared utilities & schemas for Clinic projects",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm && pnpm run build:types",
    "prepare": "pnpm run build",
    "pack:check": "pnpm pack && tar -tzf $(ls *.tgz | tail -n1)"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/longbka/types-clinic.git"
  },
  "author": "Long",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "rimraf": "^5.0.10",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "keywords": [
    "shared",
    "clinic",
    "typescript",
    "zod"
  ]
}